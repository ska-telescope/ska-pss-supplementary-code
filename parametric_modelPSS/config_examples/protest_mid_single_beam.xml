<?xml version="1.0" encoding="utf-8"?>
<cheetah>
  <beams>
    <beam>
      <active>true</active>
      <cpu>
        <affinity>0,1</affinity>
      </cpu>
      <sinks>
        <list-output-streams>false</list-output-streams>
        <threads>3</threads>
        <channels>
          <sps_events>
            <active>false</active>
            <sink>
              <id>spccl_files</id>
            </sink>
            <sink>
              <id>candidate_files</id>
            </sink>
          </sps_events>
        </channels>
        <sink_configs>
          <ocld_files>
            <extension>.ocld</extension>
            <dir>/tmp</dir>
            <id>ocld_files</id>
          </ocld_files>
          <scl_files>
            <extension>scl</extension>
            <dir>/tmp</dir>
            <id>scl_files</id>
          </scl_files>
          <sp_candidate_data>
            <extension>spccl</extension>
            <dir>/tmp</dir>
            <id>spccl_files</id>
          </sp_candidate_data>
          <spccl_files>
            <extension>.spccl</extension>
            <dir>/tmp</dir>
            <id>spccl_files</id>
          </spccl_files>
          <spccl_sigproc_files>
            <spectra_per_file>0</spectra_per_file>
            <dir>/tmp</dir>
            <extension>.fil</extension>
            <candidate_window>
              <ms_before>500</ms_before>
              <ms_after>1000</ms_after>
            </candidate_window>
            <id>candidate_files</id>
          </spccl_sigproc_files>
        </sink_configs>
      </sinks>
      <source>
        <sigproc>
          <file>test_vector_placeholder.fil</file>
          <chunk_samples>1024</chunk_samples>
          <default-nbits>8</default-nbits>
        </sigproc>
        <udp_low>
          <number_of_threads>2</number_of_threads>
          <spectra_per_chunk>2048</spectra_per_chunk>
          <number_of_channels>7776</number_of_channels>
          <max_buffers>1</max_buffers>
          <listen>
            <port>34345</port>
            <ip_address>127.0.0.1</ip_address>
          </listen>
        </udp_low>
        <udp_mid>
          <number_of_threads>2</number_of_threads>
          <spectra_per_chunk>2048</spectra_per_chunk>
          <number_of_channels>3700</number_of_channels>
          <max_buffers>1</max_buffers>
          <listen>
            <port>34345</port>
            <ip_address>127.0.0.1</ip_address>
          </listen>
        </udp_mid>
      </source>
      <id>1</id>
    </beam>
  </beams>
  <pools>
    <pool>
      <concurrent_jobs>0</concurrent_jobs>
      <cpu>
        <affinity>0</affinity>
